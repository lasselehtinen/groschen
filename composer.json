{
    "name": "lasselehtinen/groschen",
    "description": "Wrapper for Schilling Web Services",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Lasse Lehtinen",
            "email": "lasse.lehtinen@iki.fi"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ezyang/htmlpurifier": "^4.9",
        "guzzlehttp/guzzle": "~6.0",
        "fale/isbn": "^3.0",
        "league/uri": "^5.0",
        "njasm/soundcloud": "^2.2",
        "league/oauth2-client": "^2.3",
        "kamermans/guzzle-oauth2-subscriber": "^1.0.5-beta.1"
    },
    "autoload": {
        "psr-4": {
            "lasselehtinen\\Groschen\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lasselehtinen\\Groschen\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "phpro/grumphp": "^0.14.0",
        "squizlabs/php_codesniffer": "^3.1",
        "sensiolabs/security-checker": "^4.1",
        "vlucas/phpdotenv": "^2.4",
        "phpstan/phpstan": "^0.9.2",
        "orchestra/testbench": "~3.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "lasselehtinen\\Groschen\\GroschenServiceProvider"
            ],
            "aliases": {
                "Groschen": "lasselehtinen\\Groschen\\GroschenFacade"
            }
        }
    }
}
